    "@types/react-table": "^7.7.9",
    "axios": "^1.6.0",
    "class-validator": "^0.14.0",
    "clsx": "^1.2.1",
    "cross-env": "^5.2.1",
    "dotenv": "16.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "lodash": "^4.17.21",
    "medusa-interfaces": "^1.3.7",
    "medusa-react": "^9.0.5",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.38.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.13.0",
    "react-table": "^7.7.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typeorm": "^0.3.16",
    "typescript": "^4.4.4"
  },
